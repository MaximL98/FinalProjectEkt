<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="402.0" prefWidth="302.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ClientLoginController">
   <children>
      <Text layoutX="78.0" layoutY="32.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Connect to server" wrappingWidth="149.5999999999999">
         <font>
            <Font size="16.0" />
         </font></Text>
      <TextField fx:id="txtIP" layoutX="71.0" layoutY="44.0" onAction="#onEnter" />
      <Button fx:id="btnConnect" layoutX="115.0" layoutY="80.0" mnemonicParsing="false" onAction="#getConnectToServer" text="Connect" />
      <Label fx:id="hiddenLabel" alignment="CENTER" contentDisplay="CENTER" layoutX="21.0" layoutY="109.0" prefHeight="17.0" prefWidth="249.0" text="Label" textAlignment="CENTER" visible="false" />
      <ComboBox fx:id="cmbTezura" layoutX="70.0" layoutY="197.0" nodeOrientation="RIGHT_TO_LEFT" onAction="#setTezura" prefWidth="150.0" promptText="תצורה (default=OL)" />
      <ComboBox fx:id="cmbMachines" layoutX="133.0" layoutY="139.0" nodeOrientation="RIGHT_TO_LEFT" onAction="#chooseMachine" prefWidth="150.0" promptText="בחר מכונה" visible="false" />
      <Button fx:id="btnFinish" layoutX="14.0" layoutY="139.0" mnemonicParsing="false" onAction="#getFinishEkConfig" prefHeight="26.0" prefWidth="70.0" text="Finish" visible="false" />
      <CheckBox fx:id="toggleEasyRecognition" layoutX="21.0" layoutY="250.0" mnemonicParsing="false" onAction="#easyRecognitionSetter" text="Easy recognition simulator" />
      <TextField fx:id="txtFastUsr" disable="true" layoutX="71.0" layoutY="277.0" promptText="username" visible="false" />
      <TextField fx:id="txtFastPass" disable="true" layoutX="70.0" layoutY="318.0" promptText="password" visible="false" />
   </children>
</AnchorPane>
