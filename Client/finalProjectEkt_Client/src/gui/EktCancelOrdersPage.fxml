<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="800.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.EktCancelOrderPageController">
   <children>
      <Pane prefHeight="800.0" prefWidth="800.0" style="-fx-background-color: linear-gradient(from 0px 0px to 0px 1800px,#e6e6fa , INDIGO);">
         <children>
            <Pane prefHeight="120.0" prefWidth="800.0">
               <children>
                  <Text layoutX="266.0" layoutY="73.0" strokeType="OUTSIDE" strokeWidth="0.0" text="CANCEL ORDERS">
                     <font>
                        <Font name="Berlin Sans FB" size="36.0" />
                     </font>
                  </Text>
                  <ImageView fitHeight="120.0" fitWidth="200.0" pickOnBounds="true" preserveRatio="true">
                     <image>
                        <Image url="@Images/TheBetterLogo.png" />
                     </image>
                  </ImageView>
               </children>
            </Pane>
            <TableView fx:id="tableUsers" layoutX="50.0" layoutY="133.0" prefHeight="595.0" prefWidth="700.0" style="-fx-background-color: linear-gradient(from 0px 0px to 0px 1800px,#e6e6fa , INDIGO);">
              <columns>
                <TableColumn fx:id="columnOrderID" prefWidth="200.0" text="Order ID" />
                <TableColumn fx:id="columnTotalPrice" minWidth="0.0" prefWidth="200.0" text="Total Price" />
                  <TableColumn fx:id="columnDateReceived" prefWidth="200.0" text="Date Received" />
              </columns>
            </TableView>
            <Button fx:id="btnBack" layoutX="14.0" layoutY="744.0" mnemonicParsing="false" onAction="#getBtnBack" stylesheets="@buttonCSS.css" text="BACK" textFill="WHITE">
               <font>
                  <Font name="Berlin Sans FB" size="20.0" />
               </font>
            </Button>
            <Text fx:id="txtOrderCancelled" fill="#098000" layoutX="292.0" layoutY="771.0" strokeType="OUTSIDE" strokeWidth="0.0">
               <font>
                  <Font name="Berlin Sans FB" size="20.0" />
               </font>
            </Text>
         </children>
      </Pane>
   </children>
</AnchorPane>
